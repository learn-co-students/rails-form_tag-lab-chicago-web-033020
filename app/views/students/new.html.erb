<h1>Student Form:</h1>

<%= form_tag students_path do %>
  <label>Student First Name:</label><br>
  <%= text_field_tag name='student[first_name]' %><br><br>

  <label>Student Last Name:</label><br>
  <%= text_field_tag name='student[last_name]' %><br><br>

  <%= submit_tag "Submit Student" %>
<% end %>

<%= session[:form_params] if session[:form_params] %>
